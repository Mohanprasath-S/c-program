# call by value
#include <stdio.h>
int main() {
  int a,b;
  scanf("%d%d",&a,&b);
  printf("before swapping:%d %d \n",a,b);
  swap(a,b);   // call by value
  printf("after swapping :%d %d \n",a,b);
    return 0;
}
void swap(int a, int b){ // value
    int temp=a;
    a=b;
    b=temp;
    printf("after swapping :%d %d \n",a,b);
}

